  "dependencies": {
    "@use-funnel/core": "workspace:^"
  },
  "devDependencies": {
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "globals": "^15.3.0",
